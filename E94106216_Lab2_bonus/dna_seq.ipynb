    "def gc_content():\n",
    "    seq = input('DNA sequence:')\n",
    "    count = 0\n",
    "    for i in seq:\n",
    "        if i == 'G' or i == 'C':\n",
    "            count += 1\n",
    "    return count / len(seq)\n",
    "\n",
    "print(gc_content())"
